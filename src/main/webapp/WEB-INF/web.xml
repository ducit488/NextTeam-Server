<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <listener-class>nextteam.listeners.ServerContextListener</listener-class>
    </listener>
    <!--    <filter>
        <filter-name>CORS</filter-name>
        <filter-class>com.thetransactioncompany.cors.CORSFilter</filter-class>
    </filter>-->
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>nextteam.controllers.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllPublicNotificationListServlet</servlet-name>
        <servlet-class>nextteam.controllers.PublicNotificationListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PublicNotificationListSearchServlet</servlet-name>
        <servlet-class>nextteam.controllers.PublicNotificationListSearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClubServlet</servlet-name>
        <servlet-class>nextteam.controllers.ClubServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/api/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllPublicNotificationListServlet</servlet-name>
        <url-pattern>/all-public-notification-list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PublicNotificationListSearchServlet</servlet-name>
        <url-pattern>/public-notification-list-search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClubServlet</servlet-name>
        <url-pattern>/club_cmd</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
